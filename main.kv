# kv file for rendering interpretation switcher windows
# Responsible for creating basic GUI that is displayed at all times
<Background>:
    FloatLayout:

        Label:
            pos_hint:{"x":0.0, "y": 0.05}
            size_hint:0.15,0.05
            text: "Interpretations"

        Widget:
            pos_hint:{"x":0.3, "y": 0.0}
            size_hint:0.001,1
            canvas:
                Color:
                    rgba: 1,1,1,1
                Line:
                    rectangle: (self.x, self.y, self.width, self.height)

        Label:
            pos_hint:{"x":0.3, "y": 0.05}
            size_hint:0.15,0.05
            text: "Music Sheet"
